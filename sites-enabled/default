server {
        listen   80 default_server;

        root /app/lobby;
        index index.html;
	
	location /admin{
		alias /app/admin;
		index index.html;
	}
	location /vreceptionist{
		proxy_pass http://proxy:8000;
		proxy_set_header    Host            $host;
		proxy_set_header    X-Real-IP       $remote_addr;
		proxy_set_header    X-Forwarded-for $remote_addr;
	}
}
